var price=28.99;
var discount=10;
var total=
price-(price*(discount/100));
if (total>25) {
document.write ("정답");
}
else if (total>45){
document.wirte ("땡");
}



var count=10;
while (count>0) {
document.write("샤이닝<br>");
count=count-1;
}




